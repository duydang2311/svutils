{
    "name": "@duydang2311/svutils",
    "version": "0.0.8",
    "description": "A collection of Svelte utilities",
    "author": "duydang2311",
    "license": "MIT",
    "type": "module",
    "scripts": {
        "dev": "vite dev",
        "build": "vite build && npm run prepack",
        "preview": "vite preview",
        "prepare": "svelte-kit sync || echo ''",
        "prepack": "svelte-kit sync && svelte-package && publint",
        "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
        "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/duydang2311/svutils.git"
    },
    "keywords": [
        "svutils",
        "svelte",
        "utils",
        "transition",
        "gsap",
        "duydang2311"
    ],
    "bugs": {
        "url": "https://github.com/duydang2311/svutils/issues"
    },
    "homepage": "https://github.com/duydang2311/svutils#readme",
    "svelte": "./dist/index.js",
    "types": "dist/index.d.ts",
    "files": [
        "dist",
        "!dist/**/*.test.*",
        "!dist/**/*.spec.*"
    ],
    "sideEffects": [
        "**/*.css"
    ],
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "svelte": "./dist/index.js"
        }
    },
    "devDependencies": {
        "@sveltejs/adapter-auto": "^6.0.1",
        "@sveltejs/kit": "^2.22.0",
        "@sveltejs/package": "^2.3.11",
        "@sveltejs/vite-plugin-svelte": "^5.1.0",
        "publint": "^0.3.12",
        "svelte": "^5.34.7",
        "svelte-check": "^4.2.2",
        "typescript": "^5.8.3",
        "vite": "^6.3.5"
    },
    "peerDependencies": {
        "gsap": "^3.13.0",
        "svelte": "^5.0.0",
        "typescript": "^5"
    }
}
